<?php



/**
 * ContainerItem
 */
class ContainerItem
{
    /**
     * @var int
     */
    private $id;

    /**
     * @var int
     */
    private $containerid;

    /**
     * @var int
     */
    private $containertypeid;

    /**
     * @var int
     */
    private $containertypeidref;

    /**
     * @var int
     */
    private $width;

    /**
     * @var int
     */
    private $height;

    /**
     * @var bool
     */
    private $gluex;

    /**
     * @var bool
     */
    private $gluey;

    /**
     * @var bool
     */
    private $prevx;

    /**
     * @var bool
     */
    private $prevy;

    /**
     * @var bool
     */
    private $resetx;

    /**
     * @var bool
     */
    private $resety;

    /**
     * @var bool
     */
    private $drift;

    /**
     * @var bool
     */
    private $gravity;

    /**
     * @var int
     */
    private $offsetx;

    /**
     * @var int
     */
    private $offsety;


    /**
     * Get id
     *
     * @return int
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set containerid
     *
     * @param int $containerid
     *
     * @return ContainerItem
     */
    public function setContainerid($containerid)
    {
        $this->containerid = $containerid;

        return $this;
    }

    /**
     * Get containerid
     *
     * @return int
     */
    public function getContainerid()
    {
        return $this->containerid;
    }

    /**
     * Set containertypeid
     *
     * @param int $containertypeid
     *
     * @return ContainerItem
     */
    public function setContainertypeid($containertypeid)
    {
        $this->containertypeid = $containertypeid;

        return $this;
    }

    /**
     * Get containertypeid
     *
     * @return int
     */
    public function getContainertypeid()
    {
        return $this->containertypeid;
    }

    /**
     * Set containertypeidref
     *
     * @param int $containertypeidref
     *
     * @return ContainerItem
     */
    public function setContainertypeidref($containertypeidref)
    {
        $this->containertypeidref = $containertypeidref;

        return $this;
    }

    /**
     * Get containertypeidref
     *
     * @return int
     */
    public function getContainertypeidref()
    {
        return $this->containertypeidref;
    }

    /**
     * Set width
     *
     * @param int $width
     *
     * @return ContainerItem
     */
    public function setWidth($width)
    {
        $this->width = $width;

        return $this;
    }

    /**
     * Get width
     *
     * @return int
     */
    public function getWidth()
    {
        return $this->width;
    }

    /**
     * Set height
     *
     * @param int $height
     *
     * @return ContainerItem
     */
    public function setHeight($height)
    {
        $this->height = $height;

        return $this;
    }

    /**
     * Get height
     *
     * @return int
     */
    public function getHeight()
    {
        return $this->height;
    }

    /**
     * Set gluex
     *
     * @param bool $gluex
     *
     * @return ContainerItem
     */
    public function setGluex($gluex)
    {
        $this->gluex = $gluex;

        return $this;
    }

    /**
     * Get gluex
     *
     * @return bool
     */
    public function getGluex()
    {
        return $this->gluex;
    }

    /**
     * Set gluey
     *
     * @param bool $gluey
     *
     * @return ContainerItem
     */
    public function setGluey($gluey)
    {
        $this->gluey = $gluey;

        return $this;
    }

    /**
     * Get gluey
     *
     * @return bool
     */
    public function getGluey()
    {
        return $this->gluey;
    }

    /**
     * Set prevx
     *
     * @param bool $prevx
     *
     * @return ContainerItem
     */
    public function setPrevx($prevx)
    {
        $this->prevx = $prevx;

        return $this;
    }

    /**
     * Get prevx
     *
     * @return bool
     */
    public function getPrevx()
    {
        return $this->prevx;
    }

    /**
     * Set prevy
     *
     * @param bool $prevy
     *
     * @return ContainerItem
     */
    public function setPrevy($prevy)
    {
        $this->prevy = $prevy;

        return $this;
    }

    /**
     * Get prevy
     *
     * @return bool
     */
    public function getPrevy()
    {
        return $this->prevy;
    }

    /**
     * Set resetx
     *
     * @param bool $resetx
     *
     * @return ContainerItem
     */
    public function setResetx($resetx)
    {
        $this->resetx = $resetx;

        return $this;
    }

    /**
     * Get resetx
     *
     * @return bool
     */
    public function getResetx()
    {
        return $this->resetx;
    }

    /**
     * Set resety
     *
     * @param bool $resety
     *
     * @return ContainerItem
     */
    public function setResety($resety)
    {
        $this->resety = $resety;

        return $this;
    }

    /**
     * Get resety
     *
     * @return bool
     */
    public function getResety()
    {
        return $this->resety;
    }

    /**
     * Set drift
     *
     * @param bool $drift
     *
     * @return ContainerItem
     */
    public function setDrift($drift)
    {
        $this->drift = $drift;

        return $this;
    }

    /**
     * Get drift
     *
     * @return bool
     */
    public function getDrift()
    {
        return $this->drift;
    }

    /**
     * Set gravity
     *
     * @param bool $gravity
     *
     * @return ContainerItem
     */
    public function setGravity($gravity)
    {
        $this->gravity = $gravity;

        return $this;
    }

    /**
     * Get gravity
     *
     * @return bool
     */
    public function getGravity()
    {
        return $this->gravity;
    }

    /**
     * Set offsetx
     *
     * @param int $offsetx
     *
     * @return ContainerItem
     */
    public function setOffsetx($offsetx)
    {
        $this->offsetx = $offsetx;

        return $this;
    }

    /**
     * Get offsetx
     *
     * @return int
     */
    public function getOffsetx()
    {
        return $this->offsetx;
    }

    /**
     * Set offsety
     *
     * @param int $offsety
     *
     * @return ContainerItem
     */
    public function setOffsety($offsety)
    {
        $this->offsety = $offsety;

        return $this;
    }

    /**
     * Get offsety
     *
     * @return int
     */
    public function getOffsety()
    {
        return $this->offsety;
    }
}
